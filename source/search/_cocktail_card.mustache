<a href="{{url}}">
<div class="col-4 packed">
    <div class="card cocktail">
      <div class="anchor">
        <div class="preview">
          <img class="static" src="{{glass}}">
          <img class="animated" src="{{contents}}">
        </div>
      </div>
      <div class="meta center">
        <div class="label">cocktails</div>
        <div class="small-title">{{title}}</div>
{{#date}}
        <div class="small-body date">{{date}}</div>
{{/date}}
      </div>
    </div>
</div>
</a>
